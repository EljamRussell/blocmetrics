<%= javascript_include_tag "https://www.google.com/jsapi" %>

<h1><%= @registered_application.name %></h1>

<div class="row">
  <div class="col-md-8">
    <p> <%= @registered_application.url %>
      <%= link_to "Remove Registration", @registered_application, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to remove this app from registration?'} %> </p>
       <%= pie_chart @registered_application.events.group(:name).count %>
       <%= line_chart @registered_application.events.group_by_day(:created_at).count %>

    </div>

  </div>
